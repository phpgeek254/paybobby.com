{
    "name": "smodav/mpesa",
    "description": "M-Pesa API implementation",
    "type": "library",
    "keywords": ["mpesa", "safaricom", "laravel", "transactions", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "SmoDav",
            "email": "smodavprivate@gmail.com"
        }
